{
  "name": "@sinna/api",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "vitest run",
    "migrate": "node -e \"(async()=>{const{runMigrations}=await import('./dist/lib/db.js');await runMigrations();console.log('migrations done')})().catch(e=>{console.error(e);process.exit(1)})\"",
    "seed": "node dist/scripts/seed.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "@aws-sdk/s3-request-presigner": "^3.888.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.1",
    "@sentry/node": "10.17.0",
    "@sinna/types": "workspace:*",
    "bullmq": "^4.18.3",
    "fastify": "^4.27.2",
    "fastify-raw-body": "^4.3.0",
    "ioredis": "^5.7.0",
    "pg": "^8.12.0",
    "rate-limiter-flexible": "^5.0.1",
    "stripe": "^14.25.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@types/pg": "^8.15.5",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vitest": "^1.6.0"
  },
  "pnpm": {
    "overrides": {}
  }
}
