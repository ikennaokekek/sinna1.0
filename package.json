    "typescript": "^5.2.2",
    "tsx": "^3.14.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "playwright": "^1.39.0",
    "nodemon": "^3.0.1",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/sharp": "^0.32.0"
