{
  "name": "sinna-api",
  "version": "1.0.0",
  "description": "Advanced accessibility features API for streaming platforms",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/worker.ts",
