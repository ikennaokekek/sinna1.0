    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "bullmq": "^4.15.0",
    "ioredis": "^5.3.2",
    "stripe": "^14.7.0",
    "winston": "^3.11.0",
    "@sentry/node": "^7.77.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "rate-limiter-flexible": "^3.0.8",
    "cloudinary": "^1.41.0",
    "fluent-ffmpeg": "^2.1.2",
